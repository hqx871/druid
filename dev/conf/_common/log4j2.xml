<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="WARN" packages="org.apache.druid.java.util.common.logger">
  <Appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout>
        <pattern>%d{dd/MMM/yyyy HH:mm:ss,SSS} [%t] %-5level %logger{36} - %c{1}: %m%n</pattern>
      </PatternLayout>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="org.apache.druid.java.util.emitter.core.LoggingEmitter" level="info" additivity="false">
      <AppenderRef ref="STDOUT"/>
    </Logger>
    <Logger name="root" level="info">
      <AppenderRef ref="STDOUT"/>
    </Logger>
  </Loggers>
</Configuration>